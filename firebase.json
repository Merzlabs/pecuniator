{
  "hosting": [{
    "public": "www",
    "taget": "app",
    "site": "merzlabs-pecuniator",
    "ignore": [
      "firebase.json",
      "**/.*",
      "/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [ {
      "source": "**",
      "headers": [ {
        "key": "Access-Control-Allow-Origin",
        "value": "*"
      } ]
    }]
  },
  {
    "public": "pecuniator-widgets/dist",
    "taget": "widgets",
    "site": "pecuniator-widgets",
    "ignore": [
      "firebase.json",
      "**/.*",
      "/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/collection/collection-manifest.json"
      }
    ],
    "headers": [ {
      "source": "**",
      "headers": [ {
        "key": "Access-Control-Allow-Origin",
        "value": "*"
      } ]
    }]
  }
  ]
}
