{
  "hosting": [{
    "public": "www",
    "taget": "app",
    "site": "merzlabs-pecuniator",
    "ignore": [
      "firebase.json",
      "**/.*",
      "/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  {
    "public": "pecuniator-widgets/dist",
    "taget": "widgets",
    "site": "pecuniator-widgets",
    "ignore": [
      "firebase.json",
      "**/.*",
      "/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
  ]
}
